{
  "name": "rate-my-sitter",
  "version": "1.0.0",
  "description": "This is a user login and registration app using Node.js, Express, Passport, MySql and Handlebars.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "build": "concurrently \"cd client && npm run build\" \"npm build \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd ./client && npm start ",
    "server-prod": "node app.js",
    "server": "nodemon app.js",
    "start": "node app.js",
    "start:dev": "concurrently --kill-others  \"npm run client\" \"npm run server\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbardoi/rate-my-sitter.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "http-proxy": "^1.18.0",
    "http-proxy-middleware": "^1.0.1",
    "if-env": "^1.0.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "proxy-addr": "^2.0.6",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "sequelize": "^5.21.5",
    "terser": "^4.6.6"
  },
  "bugs": {
    "url": "https://github.com/nbardoi/rate-my-sitter/issues"
  },
  "homepage": "https://github.com/nbardoi/rate-my-sitter#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0"
  },
  "keywords": []
}
